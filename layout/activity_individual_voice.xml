<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFEC">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#B5D692"
        android:text="SOM"
        android:paddingLeft="15dp"
        android:textColor="#ffffff"
        tools:ignore="MissingConstraints"
        android:textSize="40dp"
        android:textStyle="bold"
        android:typeface="serif">
    </TextView>

    <TextView
        android:layout_width="150dp"
        android:layout_height="59dp"
        android:layout_gravity="center"
        android:text="1:1 음성 상담"
        android:textSize="20dp"
        android:gravity="center"
        android:typeface="serif"
        android:textColor="#67804d"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="130dp"
        tools:layout_editor_absoluteY="107dp"></TextView>

    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:src="@drawable/som8"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="274dp"
        tools:layout_editor_absoluteY="307dp" />

    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:src="@drawable/micro"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="165dp"
        tools:layout_editor_absoluteY="307dp" />

    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:src="@drawable/som1"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="307dp" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:ignore="MissingConstraints">
    </LinearLayout>

    <Button
        android:id="@+id/BtnOut"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="나가기"
        android:background="#B5D692"
        android:textSize="15dp"
        android:onClick="onClick"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="155dp"
        tools:layout_editor_absoluteY="565dp">

    </Button>
</androidx.constraintlayout.widget.ConstraintLayout>